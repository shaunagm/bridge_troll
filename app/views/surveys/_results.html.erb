<table class="table survey-results responsive-table">
  <thead>
    <tr>
      <th>Favorite things?</th>
      <th>Least favorite thing?</th>
      <th>Still want to learn:</th>
      <th title="Likelihood of participating in FLOSS">#</th>
      <th>Comments</th>
      <th>Use quotes?</th>
    </tr>
  </thead>
  <% rsvps.each do |rsvp| %>
    <% if rsvp.survey %>
      <% survey = rsvp.survey %>
      <tr>
        <td data-label="What was great:"><%= survey.good_things%></td>
        <td data-label="What could have been better:"><%= survey.bad_things %></td>
        <td data-label="What I still want to learn:"><%= survey.want_to_learn %></td>
        <td data-label="Participation likelihood:"><%= survey.participate_likelihood %></td>
        <td data-label="Comments:"><%= survey.other_comments %></td>
	<td data-label="Use of quotes:"><%= survey.use_quotes %></td>
      </tr>
    <% end %>
  <% end %>
</table>
